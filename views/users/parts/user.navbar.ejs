<div class="navbarGrace">
    <div class="container-fluid">
        <nav class="navbar navbar-expand-lg navbar-light bg-light">
            <a class="navbar-brand" href="/">
                <img src="../../../images/icons/logo.png" alt="Logo">
            </a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
                aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>

            <div class="collapse navbar-collapse" id="navbarSupportedContent">


                <ul class="navbar-nav ml-auto nav-actions">
                    <% if (typeof categories !='undefined' ) { for (let index=0; index < categories.length; index++) {
                        %>



                        <li class="nav-item">
                            <a class="nav-link" href="/category/<%= categories[index]._id %>">
                                <%= categories[index].name %></span>
                            </a>
                        </li>

                        </li>
                        <% }} %>
                            <% if (typeof isUser=='undefined' ) { %>

                                <li class="nav-item actionBtn">
                                    <a class="nav-link" href="/login">Login</span></a>
                                </li>
                                <li class="nav-item actionBtn">
                                    <a class="nav-link" href="/register">Sing up</a>
                                </li>
                                <% } else { %>
                                    <li class="nav-item actionBtn">
                                        <a class="nav-link" href="/logout">Logout</span></a>
                                    </li>
                                    <% } %>
                                        <li class="nav-item">
                                            <a class="nav-link" href="#">
                                                <img src="../../../images/icons/icon.png" alt="Cart Icon">
                                            </a>
                                        </li>
                </ul>
            </div>
        </nav>
    </div>
</div>